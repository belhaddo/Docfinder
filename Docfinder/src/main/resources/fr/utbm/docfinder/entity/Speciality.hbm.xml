<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="fr.utbm.docfinder.entity.Speciality" optimistic-lock="version" schema="DOCFINDER" table="SPECIALITY"/>
  <id column="id_spec" name="id" type="int">
    <generator class="assigned"/>
  </id>
  <property column="name_spec" name="name" type="string"/>
  <property column="desc_spec" name="desc" type="string"/>
</hibernate-mapping>
