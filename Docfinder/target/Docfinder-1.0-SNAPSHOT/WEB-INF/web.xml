        <servlet-name>Singup</servlet-name>
        <url-pattern>/singup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DispClient</servlet-name>
        <url-pattern>/dispClient</url-pattern>
